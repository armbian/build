+	    IEEE80211_HT_MAX_AMPDU_16K;
+	epub->wl.sbands[NL80211_BAND_2GHZ].ht_cap.ampdu_density =
+	    IEEE80211_HT_MPDU_DENSITY_NONE;
+	memset(&epub->wl.sbands[NL80211_BAND_2GHZ].ht_cap.mcs, 0,
