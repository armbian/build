+	u32 check_data;
+} __packed;
+
+//general debug command
+struct sip_cmd_debug {
+	u32 cmd_type;
+	u32 para_num;
+	u32 para[10];
+} __packed;
+
+struct sip_evt_debug {
+	u16 len;
+	u32 results[12];
+	u16 pad;
+} __packed;
+
+struct sip_cmd_ate {
+	//u8  len;
+	u8 cmdstr[0];
+} __packed;
+
+
+
+#endif				//ifdef TEST_MODE
+
+#endif				/* _SIP_COMMON_H_ */
