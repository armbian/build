+};
+
+enum {
+	LED_DOT2_APPS,
+	LED_DOT2_SETUP,
+	LED_DOT2_USB,
+	LED_DOT2_CARD,
+	LED_DOT2_SEC,
+	LED_DOT2_HDMI,
+	LED_DOT2_CVBS,
+	LED_DOT2_MAX
+};
+
+enum {
+	LED_DOT3_UNUSED1,
+	LED_DOT3_UNUSED2,
+	LED_DOT3_POWER,
+	LED_DOT3_LAN,
