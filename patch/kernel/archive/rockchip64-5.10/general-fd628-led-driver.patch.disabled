+module_param_array(vfd_gpio0, byte, &vfd_gpio0_argc, 0000);
+module_param_array(vfd_gpio1, byte, &vfd_gpio1_argc, 0000);
+module_param_array(vfd_gpio2, byte, &vfd_gpio2_argc, 0000);
