+};
+
+enum {
+	DISPLAY_TYPE_5D_7S_NORMAL,	// T95U
+	DISPLAY_TYPE_5D_7S_T95,		// T95K is different.
+	DISPLAY_TYPE_5D_7S_X92,
+	DISPLAY_TYPE_5D_7S_ABOX,
