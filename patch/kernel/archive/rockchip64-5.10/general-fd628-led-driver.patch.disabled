+	DISPLAY_TYPE_5D_7S_TAP1,
+	DISPLAY_TYPE_MAX,
+};
+
+#define DISPLAY_FLAG_TRANSPOSED	0x01
+#define DISPLAY_FLAG_TRANSPOSED_INT	0x00010000
+#define DISPLAY_FLAG_LOW_FREQ		0x40
+#define DISPLAY_FLAG_LOW_FREQ_INT	0x00400000
+
+enum {
+	LED_DOT1_ALARM,
+	LED_DOT1_USB,
+	LED_DOT1_PLAY,
+	LED_DOT1_PAUSE,
