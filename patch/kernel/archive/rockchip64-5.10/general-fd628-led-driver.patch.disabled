+};
+
+enum {
+	LED_DOT4_BT,
+	LED_DOT4_ETH,
+	LED_DOT4_WIFI,
+	LED_DOT4_SPDIF,
+	LED_DOT4_SEC,
+	LED_DOT4_HDMI,
+	LED_DOT4_AV,
+	LED_DOT4_MAX
+};
+
+#define LED_DOT_SEC	LED_DOT1_SEC
+#define LED_DOT_MAX	LED_DOT1_MAX
+
+static const u_int8 ledDots[LED_DOT_MAX] = {
+	0x01,
