+    .alpha2 = "99",
+    .reg_rules = {
+        REG_RULE(2412 - 10, 2472 + 10, 40, 0, 1000, 0),
+        REG_RULE(2484 - 10, 2484 + 10, 20, 0, 1000, 0),
+        REG_RULE(5150 - 10, 5850 + 10, 80, 0, 1000, 0),
