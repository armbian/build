+/** 2K buf size */
+#define BL_TX_DATA_BUF_SIZE_16K        16*1024
+#define BL_SDIO_MP_AGGR_PKT_LIMIT_MAX 	8
+#define BL_SDIO_MPA_ADDR_BASE			0x1000
+
