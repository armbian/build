+void bl_get_rd_len(struct bl_hw *bl_hw, u32 reg_l, u32 reg_u, u32 *len)
+{
+	u8 rd_len_l = 0;
+	u8 rd_len_u = 0;
+	
+	bl_read_reg(bl_hw, reg_l, &rd_len_l);
+	bl_read_reg(bl_hw, reg_u, &rd_len_u);
+
