+static inline uint8_t passive_scan_flag(uint32_t flags) {
+    if (flags & (IEEE80211_CHAN_NO_IR | IEEE80211_CHAN_RADAR))
+        return SCAN_PASSIVE_BIT;
+    return 0;
+}
