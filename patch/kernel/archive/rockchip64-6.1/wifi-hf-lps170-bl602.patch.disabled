+      BL_TYPE_DBG_RBD_DESC,
+      BL_TYPE_DBG_TX_DESC,
+      BL_TYPE_DUMP_INFO,
+      BL_TYPE_TX_STOP,
+      BL_TYPE_TX_RESUME,
+      BL_TYPE_MAX,
+};
+
