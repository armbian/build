+    ret = bl_io_read(card, response, BOOTROM_CMD_BUFFER_SIZE);
+    if (ret) {
+        printk("NULL response get");
+        return -1;
+    }
+    ret = bl_bootrom_cmd_checkimage_get_res((bootrom_res_checkimage_t*)response);
+    if (ret) {
+        printk("Error response get checkimage\n");
