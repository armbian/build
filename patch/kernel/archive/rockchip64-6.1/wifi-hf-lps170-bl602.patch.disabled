+    txq = bl_txq_sta_get(bl_sta, 0, &idx, bl_hw);
+    for (tid = 0; tid < NX_NB_TXQ_PER_STA; tid++, txq++, idx++) {
+        bl_txq_init(txq, idx, status, &bl_hw->hwq[bl_tid2hwq[tid]],
+                      bl_sta, bl_vif->ndev);
+        txq->ps_id = bl_sta->uapsd_tids & (1 << tid) ? UAPSD_ID : LEGACY_PS_ID;
