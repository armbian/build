+void main_wq_hdlr(struct work_struct *work)
+{
+	struct bl_hw *bl_hw = container_of(work, struct bl_hw, main_work);
+
+	BL_DBG(BL_FN_ENTRY_STR);
+	
+	bl_main_process(bl_hw);
+}
