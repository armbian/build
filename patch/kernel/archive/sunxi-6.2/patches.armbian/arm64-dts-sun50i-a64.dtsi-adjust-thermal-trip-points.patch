From cbf6d09b1bd690775f6c24a9742055e58be7f9bb Mon Sep 17 00:00:00 2001
From: Stefan Saraev <stefan@saraev.ca>
Date: Mon, 6 Apr 2020 15:26:10 +0300
Subject: [PATCH 128/153] arm64:dts:sun50i-a64.dtsi adjust thermal trip points

default values for alert1/crit are way too high.
---
 arch/arm64/boot/dts/allwinner/sun50i-a64.dtsi | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm64/boot/dts/allwinner/sun50i-a64.dtsi b/arch/arm64/boot/dts/allwinner/sun50i-a64.dtsi
index 6cd8e04ab..e7095e514 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-a64.dtsi
+++ b/arch/arm64/boot/dts/allwinner/sun50i-a64.dtsi
@@ -244,21 +244,21 @@ map1 {
 			trips {
 				cpu_alert0: cpu_alert0 {
 					/* milliCelsius */
-					temperature = <75000>;
+					temperature = <70000>;
 					hysteresis = <2000>;
 					type = "passive";
 				};
 
 				cpu_alert1: cpu_alert1 {
 					/* milliCelsius */
-					temperature = <90000>;
+					temperature = <80000>;
 					hysteresis = <2000>;
 					type = "hot";
 				};
 
 				cpu_crit: cpu_crit {
 					/* milliCelsius */
-					temperature = <110000>;
+					temperature = <90000>;
 					hysteresis = <2000>;
 					type = "critical";
 				};
-- 
