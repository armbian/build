+	dc-gpios  = <&pio 2 14 GPIO_ACTIVE_HIGH>; /* PC14 */
+};
+
+&spi1 {
+	cs-gpios = <&pio 2 11 GPIO_ACTIVE_HIGH>, /* PC11 */
