@@ -960,6 +1085,26 @@ &i2c5 {
 	i2c-scl-falling-time-ns = <15>;
 	pinctrl-0 = <&i2c5_xfer &pogo_int_pin>;
 	status = "okay";
+
+	ppkb: keyboard@15 {
+		compatible = "pine64,pinephone-keyboard";
+		reg = <0x15>;
+		interrupt-parent = <&gpio3>;
+		interrupts = <RK_PA0 IRQ_TYPE_EDGE_FALLING>;
+		vbat-supply = <&vcc5v0_sys>;
+		wakeup-source;
+
+		i2c {
+			#address-cells = <1>;
+			#size-cells = <0>;
