       - rockchip,rk3228-vpu
       - rockchip,rk3288-vpu
       - rockchip,rk3328-vpu
@@ -36,12 +37,16 @@ properties:
           - const: vdpu
 
   clocks:
-    maxItems: 2
+    maxItems: 4
 
   clock-names:
     items:
       - const: aclk
+      - const: aclk_vdpu
+      - const: aclk_vepu
       - const: hclk
+      - const: hclk_vdpu
+      - const: hclk_vepu
 
   power-domains:
