+#set -x
+	check_dep "deb"
+	if [[ $? -ne 0 ]];then exit 1;fi
+	get_version
+	ver=${kernver}-${board}${gitbranch}
+	uimagename=uImage_${kernver}${gitbranch}
+	echo "deb package ${ver}"
+	prepare_SD
