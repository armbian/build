	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <26500000>;
		m25p,fast-read;
		broken-flash-reset;
		status = "okay";

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@00000 {
				label = "bootinfo";
				reg = <0x00000 0x10000>;
			};

			partition@10000 {
				label = "private";
				reg = <0x10000 0x10000>;
			};

			partition@20000 {
				label = "fsbl";
				reg = <0x20000 0x40000>;
			};

			partition@60000 {
				label = "env";
				reg = <0x60000 0x10000>;
			};

			partition@70000 {
				label = "opensbi";
				reg = <0x70000 0x30000>;
			};

			partition@a0000 {
				label = "uboot";
				reg = <0xa0000 0xf60000>;
			};
		};
	};
};

