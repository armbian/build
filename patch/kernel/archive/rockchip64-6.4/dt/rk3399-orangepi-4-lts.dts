};

&i2c1 {
	clock-frequency = <200000>;
	i2c-scl-rising-time-ns = <300>;
	i2c-scl-falling-time-ns = <15>;
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	es8316c_codec: es8316c@11 {
		#sound-dai-cells = <0>;
		compatible = "everest,es8316";
		reg = <17>;
		clocks = <&cru SCLK_I2S_8CH_OUT>;
		clock-names = "mclk";
		//pinctrl-names = "default";
		//pinctrl-0 = <&i2s_8ch_mclk>;
		status = "okay";
	};

};

&i2c3 {
	status = "okay";
