@@ -829,6 +829,11 @@ spdif_tx_pin: spdif-tx-pin {
 				function = "spdif";
 			};
 
+			i2s0_pins: i2s0-pins {
+				pins = "PB4", "PB5", "PB6", "PB7", "PB8";
+				function = "i2s0";
+			};
