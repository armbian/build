Subject: [PATCH] Fix: depends only ARM: eInk display FB

---
 drivers/video/fbdev/Kconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/drivers/video/fbdev/Kconfig b/drivers/video/fbdev/Kconfig
index 3b52fedda..e84fe7d1a 100644
--- a/drivers/video/fbdev/Kconfig
+++ b/drivers/video/fbdev/Kconfig
@@ -872,7 +872,7 @@ config FB_ATMEL
 
 config FB_SUN5I_EINK
 	tristate "eInk display Framebuffer Support (A13 based eBook readers)"
-	depends on FB
+	depends on FB && ARM && !ARM64
 	select FB_MODE_HELPERS
 	select FB_CFB_FILLRECT
 	select FB_CFB_COPYAREA
