From ad5ef362f1072ea3817a85be44d7a06265dd89e9 Mon Sep 17 00:00:00 2001
From: Ondrej Jirman <megous@megous.com>
Date: Thu, 21 Oct 2021 20:24:34 +0200
Subject: [PATCH 107/467] arm64: dts: rk3399: Disable debug nodes (they hang
 boot)

Signed-off-by: Ondrej Jirman <megous@megous.com>
---
 arch/arm64/boot/dts/rockchip/rk3399.dtsi | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3399.dtsi b/arch/arm64/boot/dts/rockchip/rk3399.dtsi
index d3cdf6f42..a75e4eeb8 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3399.dtsi
@@ -373,6 +373,7 @@ usb_host1_ohci: usb@fe3e0000 {
 		status = "disabled";
 	};
 
+/*
 	debug@fe430000 {
 		compatible = "arm,coresight-cpu-debug", "arm,primecell";
 		reg = <0 0xfe430000 0 0x1000>;
@@ -420,6 +421,7 @@ debug@fe710000 {
 		clock-names = "apb_pclk";
 		cpu = <&cpu_b1>;
 	};
+*/
 
 	usbdrd3_0: usb@fe800000 {
 		compatible = "rockchip,rk3399-dwc3";
-- 
2.34.0

