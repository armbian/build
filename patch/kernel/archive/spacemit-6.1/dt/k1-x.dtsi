			mmu-type = "riscv,sv39";
			cpu-idle-states = <&CPU_NONRET> /*, <&CLUSTER_NONRET>, <&TOP_NONRET> */;
			#cooling-cells = <2>;

			cpu1_intc: interrupt-controller {
