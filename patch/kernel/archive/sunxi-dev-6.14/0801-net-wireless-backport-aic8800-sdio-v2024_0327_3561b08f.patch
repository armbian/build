+
+       if((RWNX_VIF_TYPE(vif) != NL80211_IFTYPE_AP) && (RWNX_VIF_TYPE(vif) != NL80211_IFTYPE_P2P_GO) &&
+               (RWNX_VIF_TYPE(vif) != NL80211_IFTYPE_AP_VLAN))
+               return -EINVAL;
+	   spin_lock_bh(&vif->rwnx_hw->cb_lock);
