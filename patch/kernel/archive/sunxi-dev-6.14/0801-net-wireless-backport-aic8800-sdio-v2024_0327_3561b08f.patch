+#endif
+
+	#ifdef CONFIG_PREALLOC_RX_SKB
+	aicwf_prealloc_exit();
+	#endif
+	kfree(rx_priv);
+
+	AICWFDBG(LOGINFO, "%s exit \n", __func__);
+}
