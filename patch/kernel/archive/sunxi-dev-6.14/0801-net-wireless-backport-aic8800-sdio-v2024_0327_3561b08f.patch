+	&dev_attr_hwinfo.attr,
+	&dev_attr_fwdebug.attr,
+	NULL,
+};
+
+static struct attribute_group aicbsp_attribute_group = {
+	.name  = "aicbsp_info",
+	.attrs = aicbsp_attributes,
+};
