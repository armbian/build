+static inline
+s8 rwnx_txq_get_credits(struct rwnx_txq *txq)
+{
+	s8 cred = txq->credits;
+	/* if destination is in PS mode, push_limit indicates the maximum
+	   number of packet that can be pushed on this txq. */
+	if (txq->push_limit && (cred > txq->push_limit)) {
+		cred = txq->push_limit;
+	}
+	return cred;
