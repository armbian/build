+		aicwf_sdio_pwrctl_timer(sdiodev, 0);
+		return ret;
+	}
+
+	if (sdiodev->state == SDIO_ACTIVE_ST) {
+		{
+			sdio_dbg("s\n");
+			ret = aicwf_sdio_writeb(sdiodev, sdiodev->sdio_reg.sleep_reg, 0x10);
+			if (ret)
+				sdio_err("Write sleep fail!\n");
+		}
+		sdiodev->state = SDIO_SLEEP_ST;
+		aicwf_sdio_pwrctl_timer(sdiodev, 0);
