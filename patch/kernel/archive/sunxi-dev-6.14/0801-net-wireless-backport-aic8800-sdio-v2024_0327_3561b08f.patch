+static inline bool use_pairwise_key(struct cfg80211_crypto_settings *crypto)
+{
+	if ((crypto->cipher_group ==  WLAN_CIPHER_SUITE_WEP40) ||
+		(crypto->cipher_group ==  WLAN_CIPHER_SUITE_WEP104))
+		return false;
+
+	return true;
+}
