+static int isxdigit(unsigned char c)
+{
+	if ((c >= '0') && (c <= '9'))
+		return 1;
+	if ((c >= 'a') && (c <= 'f'))
+		return 1;
+	if ((c >= 'A') && (c <= 'F'))
