+
+uint8_t crc8_ponl_107(uint8_t *p_buffer, uint16_t cal_size)
+{
+    uint8_t i;
+    uint8_t crc = 0;
+    if (cal_size==0) {
+        return crc;
+    }
+    while (cal_size--) {
+        for (i = 0x80; i > 0; i /= 2) {
+            if (crc & 0x80)  {
+                crc *= 2;
+                crc ^= 0x07; //polynomial X8 + X2 + X + 1,(0x107)
+            } else {
+                crc *= 2;
+            }
+            if ((*p_buffer) & i) {
+                crc ^= 0x07;
+            }
+        }
+        p_buffer++;
+    }
+    return crc;
+}
