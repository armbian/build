+			/// Whether the old probability is available
+			bool_l  old_prob_available;
+			/// Whether the rate can be used in the retry chain
+			bool_l rate_allowed;
+		};
+		struct {
+			/// RU size and UL length received in the latest HE trigger frame
+			u16_l ru_and_length;
+		};
+	};
+};
