+	ret = aicwf_sdio_readb(sdiodev, sdiodev->sdio_reg.sleep_reg, &val);
+	if (ret < 0) {
+		AICWFDBG(LOGERROR, "reg:%d read failed!\n", sdiodev->sdio_reg.sleep_reg);
+		return ret;
+	}
+
+	if(!(val & 0x10)){
+		AICWFDBG(LOGERROR, "wakeup fail\n");
+	}else{
+		AICWFDBG(LOGINFO, "sdio ready\n");
+	}
+#endif
+	return ret;
+}
