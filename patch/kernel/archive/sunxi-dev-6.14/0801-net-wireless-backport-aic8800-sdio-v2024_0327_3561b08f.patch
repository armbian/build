diff --speed-large-files --no-dereference --minimal -Naur linux-6.9.4/drivers/net/wireless/aic8800_sdio/aic8800_fdrv/ipc_host.h linux-6.9.4/drivers/net/wireless/aic8800_sdio/aic8800_fdrv/ipc_host.h
