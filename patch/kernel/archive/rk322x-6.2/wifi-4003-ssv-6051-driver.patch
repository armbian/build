+		if (SSV_REG_READ1
+		    (ssv6xxx_debug_ifops, ADR_DBG_AMPDU_FAIL, &value)) {
+			sprintf(temp_str, "[%08x]\t\t", value);
+			strcat(ssv6xxx_result_buf, temp_str);
