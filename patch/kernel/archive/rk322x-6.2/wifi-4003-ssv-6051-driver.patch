+#define MCS_NBITS (AVG_PKT_SIZE << 3)
+#define MCS_NSYMS(bps) ((MCS_NBITS + (bps) - 1) / (bps))
+#define MCS_SYMBOL_TIME(sgi,syms) \
+    (sgi ? \
+      ((syms) * 18 + 4) / 5 : \
+      (syms) << 2 \
