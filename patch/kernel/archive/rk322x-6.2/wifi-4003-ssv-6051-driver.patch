+		sprintf(pointer, "MCS2-s");
+		return;
+	case 26:
+		sprintf(pointer, "MCS3-s");
+		return;
+	case 27:
+		sprintf(pointer, "MCS4-s");
+		return;
+	case 28:
+		sprintf(pointer, "MCS5-s");
+		return;
+	case 29:
+		sprintf(pointer, "MCS6-s");
+		return;
+	case 30:
+		sprintf(pointer, "MCS7-s");
+		return;
+	default:
+		sprintf(pointer, "Unknow");
+		return;
+	};
