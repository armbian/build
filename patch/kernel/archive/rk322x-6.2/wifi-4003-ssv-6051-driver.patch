+static void ssv6xxx_rate_free_sta(void *priv, struct ieee80211_sta *sta,
+				  void *priv_sta)
+{
+	struct ssv_sta_rc_info *rc_sta = priv_sta;
+	rc_sta->rc_valid = false;
+}
