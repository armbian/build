+static void ssv6xxx_rate_free(void *priv)
+{
+	struct ssv_softc *sc = priv;
+	if (sc->rc) {
+		kfree(sc->rc);
+		sc->rc = NULL;
+	}
+	sc->rc_sample_sechedule = 0;
+	cancel_work_sync(&sc->rc_sample_work);
+	flush_workqueue(sc->rc_sample_workqueue);
+	destroy_workqueue(sc->rc_sample_workqueue);
+}
