+void ssv6200_ampdu_tx_operation(u16 tid, struct ieee80211_sta *sta,
+				struct ieee80211_hw *hw, u8 buffer_size)
+{
+    struct ssv_softc *sc = hw->priv;
+	struct ssv_sta_priv_data *ssv_sta_priv;
