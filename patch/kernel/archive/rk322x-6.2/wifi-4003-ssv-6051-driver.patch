+void ssv6xxx_deinit_mac(struct ssv_softc *sc)
+{
+	int i;
+	for (i = 0; i < SSV_RC_MAX_STA; i++) {
+		if (sc->sh->hw_buf_ptr[i])
+			ssv6xxx_pbuf_free(sc, sc->sh->hw_buf_ptr[i]);
+	}
+}
