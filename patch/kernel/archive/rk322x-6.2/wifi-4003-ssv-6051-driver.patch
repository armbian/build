+static int ssv6xxx_sdio_irq_getmask(struct device *child, u32 * mask)
+{
+	u8 imask = 0;
+	int ret = (-1);
+	struct ssv6xxx_sdio_glue *glue = dev_get_drvdata(child->parent);
