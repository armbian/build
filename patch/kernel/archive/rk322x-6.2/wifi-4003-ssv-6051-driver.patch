+static void _clear_mpdu_q(struct ieee80211_hw *hw, struct sk_buff_head *q,
+			  bool aggregated_mpdu)
+{
+	struct sk_buff *skb;
+	while (1) {
+		skb = skb_dequeue(q);
+		if (!skb)
+			break;
+		if (aggregated_mpdu)
+			skb_pull(skb, AMPDU_DELIMITER_LEN);
+		ieee80211_tx_status(hw, skb);
+	}
+}
