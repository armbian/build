+	struct ssv_vif_priv_data *vif_priv =
+	    (struct ssv_vif_priv_data *)vif_info->vif->drv_priv;
+	sta_priv->has_hw_encrypt = vif_priv->has_hw_encrypt;
+	sta_priv->has_hw_decrypt = vif_priv->has_hw_decrypt;
