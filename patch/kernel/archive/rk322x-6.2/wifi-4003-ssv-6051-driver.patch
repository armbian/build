+		if (SSV_REG_READ1(ssv6xxx_debug_ifops, 0xCE0023EC, &value)) {
+			sprintf(temp_str, "[%08x]\t\t", (value >> 16) & 0xffff);
+			strcat(ssv6xxx_result_buf, temp_str);
+			sprintf(temp_str, "[%08x]\t\t\n\n", value & 0xffff);
