+static void ssv6xxx_sdio_irq_trigger(struct device *child)
+{
+	int err_ret;
+	struct ssv6xxx_sdio_glue *glue = dev_get_drvdata(child->parent);
+	struct sdio_func *func;
+
+    ret_if_not_ready();
+
+    func = dev_to_sdio_func(glue->dev);
+    sdio_claim_host(func);
