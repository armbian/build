+			compression = "gzip";
+			entry = <0x02080000>;
+			load = <0x02080000>;
+			hash {
+				algo = "sha256";
+			};
+		};
+		fdt {
+			description = "Flattened Device Tree blob";
+			data = /incbin/("arch/arm64/boot/dts/rockchip/rk3568-bpi-r2-pro.dtb");
+			type = "flat_dt";
