From 187a46f48847b51251873229c394e28003c86dc6 Mon Sep 17 00:00:00 2001
From: Andre Przywara <andre.przywara@arm.com>
Date: Sat, 28 Nov 2020 19:50:08 +0000
Subject: [PATCH 11/13] .dtsi: Use A100 fallback for MMC compatible string

---
 arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi b/arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi
index 12b2b6f75587..9b9c20b83b3f 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi
+++ b/arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi
@@ -261,7 +261,7 @@ gic: interrupt-controller@3021000 {
 
 		mmc0: mmc@4020000 {
 			compatible = "allwinner,sun50i-h616-mmc",
-				     "allwinner,sun50i-a64-mmc";
+				     "allwinner,sun50i-a100-mmc";
 			reg = <0x04020000 0x1000>;
 			clocks = <&ccu CLK_BUS_MMC0>, <&ccu CLK_MMC0>;
 			clock-names = "ahb", "mmc";
@@ -277,7 +277,7 @@ mmc0: mmc@4020000 {
 
 		mmc1: mmc@4021000 {
 			compatible = "allwinner,sun50i-h616-mmc",
-				     "allwinner,sun50i-a64-mmc";
+				     "allwinner,sun50i-a100-mmc";
 			reg = <0x04021000 0x1000>;
 			clocks = <&ccu CLK_BUS_MMC1>, <&ccu CLK_MMC1>;
 			clock-names = "ahb", "mmc";
-- 
2.25.1

