-    maxItems: 2
+    maxItems: 4
 
   clock-names:
     items:
       - const: aclk
+      - const: aclk_vdpu
+      - const: aclk_vepu
       - const: hclk
+      - const: hclk_vdpu
+      - const: hclk_vepu
 
