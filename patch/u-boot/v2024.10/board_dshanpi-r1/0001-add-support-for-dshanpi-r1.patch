Date: Sun, 22 Dec 2024 16:27:57 +0800
Subject: [PATCH] add support for dshanpi-r1

---
 arch/arm/dts/rk3568-dshanpi-r1-u-boot.dtsi    |  42 ++
 configs/dshanpi-r1-rk3568_defconfig           |  93 +++
 .../src/arm64/rockchip/rk3568-dshanpi-r1.dts  | 689 ++++++++++++++++++
 3 files changed, 824 insertions(+)
 create mode 100644 arch/arm/dts/rk3568-dshanpi-r1-u-boot.dtsi
 create mode 100644 configs/dshanpi-r1-rk3568_defconfig
 create mode 100644 dts/upstream/src/arm64/rockchip/rk3568-dshanpi-r1.dts

diff --git a/arch/arm/dts/rk3568-dshanpi-r1-u-boot.dtsi b/arch/arm/dts/rk3568-dshanpi-r1-u-boot.dtsi
new file mode 100644
index 00000000..869724ec
--- /dev/null
+++ b/arch/arm/dts/rk3568-dshanpi-r1-u-boot.dtsi
@@ -0,0 +1,42 @@
+// SPDX-License-Identifier: GPL-2.0+
+/*
+ * (C) Copyright 2021 Rockchip Electronics Co., Ltd
+ */
+
+#include "rk356x-u-boot.dtsi"
+
+&pcie3x2 {
+	pinctrl-0 = <&pcie3x2_reset_h>;
+};
+
+&pinctrl {
+	pcie {
+		pcie3x2_reset_h: pcie3x2-reset-h {
+			rockchip,pins = <2 RK_PD6 RK_FUNC_GPIO &pcfg_pull_none>;
+		};
+	};
+};
+
+&sdhci {
+	cap-mmc-highspeed;
+	mmc-hs200-1_8v;
+	mmc-hs400-1_8v;
+	mmc-hs400-enhanced-strobe;
+};
+
+&sfc {
+	flash@0 {
+		bootph-pre-ram;
+		bootph-some-ram;
+	};
+};
+
+&usb_host0_ohci {
+	status = "disabled";
+};
+
+&vdd_npu {
+	regulator-always-on;
+	regulator-boot-on;
+	regulator-init-microvolt = <900000>;
+};
